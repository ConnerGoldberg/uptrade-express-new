{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.ts",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.ts",
    "server": "nodemon server.ts",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --coverage",
    "test:watch": "sh server/docker/start.sh && jest --watchAll --coverage"
  },
  "author": "Conner Goldberg",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/iconv-lite": "0.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "jest": "26.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.14.6",
    "ts-jest": "^26.5.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "history": "^5.0.0",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.28.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "4.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
