    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "history": "^5.0.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "4.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0"
  }
